package main

import (
	"fmt"
	"math"
)

func main() {
	var A, B, C float64
	fmt.Scan(&A, &B, &C)
	if A == 0 {
		fmt.Println("Nao e equacao do segundo grau")
		return
	}
	D := B*B - 4*A*C
	if D < 0 {
		fmt.Println("Nao ha raizes reais")
		return
	}
	x1 := (-B + math.Sqrt(D)) / (2 * A)
	x2 := (-B - math.Sqrt(D)) / (2 * A)
	if D == 0 {
		fmt.Printf("A unica raiz real eh: %.2f\n", x1)
	} else {
		if x1 < x2 {
			x1, x2 = x2, x1
		}
		fmt.Printf("As raizes sao: %.2f e %.2f\n", x1, x2)
	}
}
